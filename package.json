{
  "name": "node-ts-prettier",
  "version": "1.0.0",
  "description": "",
  "keywords": [
    "eslint",
    "jest",
    "node",
    "prettier",
    "typescript"
  ],
  "main": "dist/index.js",
  "scripts": {
    "build:types": "$(npm bin)/tsc --emitDeclarationOnly",
    "build:js": "$(npm bin)/babel src --out-dir dist --extensions \".ts\"",
    "build": "npm run build:types && npm run build:js",
    "dev": "$(npm bin)/babel-watch src --watch --out-dir dist --extensions \".ts\"",
    "format": "$(npm bin)/prettier --write \"src/**/*.{ts,js}\"",
    "purge": "rm -rf node_modules package-lock.json; npm i",
    "start": "node dist/index.js",
    "test:lint": "$(npm bin)/eslint 'src/**/*.{ts,tsx}' --quiet",
    "test:types": "$(npm bin)/tsc --noEmit",
    "test:unit": "$(npm bin)/jest src -c ./jest.config.js",
    "test": "npm run test:types; npm run test:lint; npm run test:unit"
  },
  "author": "Jordan Papaleo",
  "license": "ISC",
  "dependencies": {
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "lodash": "^4.17.15"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.3",
    "@babel/core": "7.8.4",
    "@babel/preset-env": "7.8.3",
    "@babel/preset-typescript": "7.8.3",
    "@types/eslint": "^6.1.1",
    "@types/express": "^4.17.2",
    "@types/jest": "^25.1.3",
    "@types/lodash": "^4.14.141",
    "@types/node": "^12.7.8",
    "@typescript-eslint/eslint-plugin": "^2.20.0",
    "@typescript-eslint/parser": "^2.20.0",
    "babel-jest": "^25.1.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-watch": "^7.0.0",
    "eslint": "^6.4.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-config-typescript": "^3.0.0",
    "eslint-plugin-prettier": "^3.1.2",
    "jest": "25.1.0",
    "lint-staged": "10.0.7",
    "prettier": "^1.19.1",
    "ts-jest": "^25.2.1",
    "typescript": "^3.6.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": "npm run format"
  }
}
